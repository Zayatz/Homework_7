<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="@dimen/item_padding"
        android:bottom="@dimen/item_padding">
        <shape android:shape="rectangle">
            <solid android:color="@color/contentDisableColor"
                />
            <corners android:topRightRadius="@dimen/item_corners_radius"
                />
            <stroke android:color="@color/colorPrimaryLight"
                android:width="@dimen/item_stroke_width"
                />
        </shape>
    </item>
    <item>
        <bitmap android:src="@drawable/ic_settings"
            android:gravity="center"
            />
    </item>
</layer-list>